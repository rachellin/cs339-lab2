modules = ["rust-stable", "bash"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Build Project"

[[workflows.workflow]]
name = "Build Project"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cargo build --workspace && echo \"Build successful! Run 'cargo test --workspace' to test your implementation.\""

[workflows.workflow.metadata]
outputType = "console"
